<h1>Shopping List</h1>
<table>
  <thead>
    <tr>
      <th>Food</th>
      <th>Quantity</th>
      <th>Price</th>
    </tr>
  </thead>
  <tbody>
    <% @shopping_list[:shopping_list].each do |food_name, data| %>
      <tr>
        <td><%= food_name %></td>
        <td><%= data[:quantity] %></td>
        <td>$<%= data[:price] %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<p>Total Food Items to Buy: <%= @shopping_list[:total_items] %></p>
<p>Total Value: $<%= @shopping_list[:total_value] %></p>
